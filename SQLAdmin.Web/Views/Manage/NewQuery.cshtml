<div ng-controller="query.controller">
    <sui-code-edit exec="vm.exec(code,language)"></sui-code-edit>
    <sui-line title="'查询结果'"></sui-line>
    <div class="sa-newquery-result">
        <sui-datagrid ng-if="vm.execResult.ResultType == 0" datas="vm.execResult.Result" is-show-thead="false" is-can-select="false" isEdit="false"></sui-datagrid>
        <span ng-if="vm.execResult.ResultType == 1" class="sa-newquery-result-error">{{vm.execResult.Result}}</span>
    </div>
</div>